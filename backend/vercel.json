{
  "version": 2,
  "functions": {
    "server.js": {
      "maxDuration": 60,
      "memory": 3008,
      "maxRequestBodySize": "10mb"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}